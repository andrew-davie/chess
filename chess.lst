     29  7ce9					      ldy	currentSquare
     30  7ce9
     31  7ce9					      IF	{1} = QUEENSIDE
     32  7ce9					      lda	Board-3,y	; nothing in N pos
     33  7ce9					      bne	.noCastle
     34  7ce9					      lda	Board-2,y	; nothing in B pos
     35  7ce9					      bne	.noCastle
     36  7ce9					      lda	Board-1,y	; nothing in Q pos
     37  7ce9					      bne	.noCastle
     38  7ce9					      ENDIF
     39  7ce9
     40  7ce9					      IF	{1} = KINGSIDE
     41  7ce9					      lda	Board+2,y	; check N pos
     42  7ce9					      bne	.noCastle
     43  7ce9					      lda	Board+1,y	; check B pos
     44  7ce9					      bne	.noCastle
     45  7ce9					      ENDIF
