      0  5277					      include	"Handler_KING.asm"
      1  5277							; Copyright (C)2020 Andrew Davie
      2  5277							; Piece move handler for king
      3  5277
      4  5277
      5  5277							; This is the move handler for a KING
      6  5277							; "Check" is detected in the next ply of the search, so the move generation doesn't have to
      7  5277							; be concerned about that. To assist with castling over squares in check (which is illegal)
      8  5277							; the concept of a phantom king is introduced. Phantom kings are effectively blank squares
      9  5277							; but need to be checked when moving opposite-colour pieces to a square. Messy.
     10  5277
     11  5277							;---------------------------------------------------------------------------------------------------
     12  5277							; MACRO - Castling
     13  5277
     14  5277		       00 03	   KINGSIDE   =	3
     15  5277		       ff ff ff fc QUEENSIDE  =	-4
     16  5277
     17  5277					      MAC	castle
     18  5277							; {1} == KINGSIDE or QUEENSIDE
     19  5277					      SUBROUTINE
     20  5277
     21  5277							; Most likely failure trigger is there are pieces in the way (N or B) (or Q)
     22  5277							; Check these squares first as it's the cheapest "exit" from castle check
     23  5277
     24  5277							; Note: castling with squares that are "in check" is problematic
     25  5277							; TODO: next ply have a "phantom" king on the positions king moves over...?
     26  5277
     27  5277							; todo: incomplete... x/y regs usage etc
     28  5277
     29  5277					      ldy	currentSquare
     30  5277
     31  5277					      IF	{1} = QUEENSIDE
     32  5277					      lda	Board-3,y	; nothing in N pos
     33  5277					      bne	.noCastle
     34  5277					      lda	Board-2,y	; nothing in B pos
     35  5277					      bne	.noCastle
     36  5277					      lda	Board-1,y	; nothing in Q pos
     37  5277					      bne	.noCastle
     38  5277					      ENDIF
     39  5277
     40  5277					      IF	{1} = KINGSIDE
     41  5277					      lda	Board+2,y	; check N pos
     42  5277					      bne	.noCastle
     43  5277					      lda	Board+1,y	; check B pos
     44  5277					      bne	.noCastle
     45  5277					      ENDIF
     46  5277
     47  5277							; appropriate N/B/(Q) squares are vacant so we proceed with more checks...
     48  5277
     49  5277					      lda	Board+{1},y	; we expect a R
     50  5277					      sta	__piece
     51  5277
     52  5277					      and	#PIECE_MASK
     53  5277					      cmp	#ROOK
     54  5277					      bne	.noCastle	; not a R
     55  5277
     56  5277					      lda	__piece
     57  5277					      eor	currentPiece
     58  5277					      bmi	.noCastle	; not correct colour
     59  5277
     60  5277					      bit	__piece
     61  5277					      bvs	.noCastle	; it's previously moved so we can't castle
     62  5277
     63  5277							; FINALLY -- king can castle
     64  5277							; note: when we actually DO the move we MUST insert "Phantom" kings onto the board over the
     65  5277							; squares the king traverses so that "check" (and thus illegal moves) can be detected on the
     66  5277							; next move. Castling will be detected by K moving > 1 square.
     67  5277
     68  5277					      jsr	AddMove
     69  5277				   .noCastle
     70  5277					      ENDM
     71  5277
     72  5277
