      4  6267
      5  6267							; This is the move handler for a KING
      6  6267							; "Check" is detected in the next ply of the search, so the move generation doesn't have to
      7  6267							; be concerned about that. To assist with castling over squares in check (which is illegal)
      8  6267							; the concept of a phantom king is introduced. Phantom kings are effectively blank squares
      9  6267							; but need to be checked when moving opposite-colour pieces to a square. Messy.
     10  6267
     11  6267							;---------------------------------------------------------------------------------------------------
     12  6267							; MACRO - Castling
     13  6267
     14  6267		       00 03	   KINGSIDE   =	3
     15  6267		       ff ff ff fc QUEENSIDE  =	-4
     16  6267
     17  6267					      MAC	castle
     18  6267							; {1} == KINGSIDE or QUEENSIDE
     19  6267
     20  6267					      ldx	currentSquare
     21  6267					      lda	Board+{1},x	; kingside/queenside R position
     22  6267					      and	#PIECE_MASK|FLAG_MOVED
     23  6267					      cmp	#ROOK
     24  6267					      bne	.noCastle	; not a R that hasn't moved
     25  6267
     26  6267							; It's a R and it *HAS* to be correct colour because it hasn't moved!
     27  6267							; AND the K hasn't moved (earlier check), so check for vacant squares between K and R
     28  6267
     29  6267					      IF	{1} = QUEENSIDE
     30  6267					      lda	Board-3,x	; nothing in N pos
     31  6267					      bne	.noCastle
     32  6267					      lda	Board-2,x	; nothing in B pos
     33  6267					      bne	.noCastle
     34  6267					      lda	Board-1,x	; nothing in Q pos
     35  6267					      bne	.noCastle
     36  6267
     37  6267					      ENDIF
     38  6267
     39  6267					      IF	{1} = KINGSIDE
     40  6267					      lda	Board+2,x	; check N pos
     41  6267					      bne	.noCastle
     42  6267					      lda	Board+1,x	; check B pos
     43  6267					      bne	.noCastle
     44  6267					      ENDIF
     45  6267
     46  6267							; appropriate N/B/(Q) squares are vacant so we proceed...
     47  6267
     48  6267							; FINALLY -- king can castle
     49  6267							; note: when we actually DO the move we MUST insert "Phantom" kings onto the board over the
     50  6267							; squares the king traverses so that "check" (and thus illegal moves) can be detected on the
     51  6267							; next move. Castling will be detected by K moving > 1 square. (TODO: FIX?? not CASTLE flag??)
     52  6267
     53  6267					      lda	currentPiece
     54  6267					      ora	#FLAG_CASTLE	; flag it's a castling move
     55  6267					      sta	currentPiece
     56  6267
     57  6267					      ldy	ValidSquare+{2},x
     58  6267
     59  6267					      jsr	AddMove
     60  6267
     61  6267				   .noCastle
     62  6267					      ENDM
