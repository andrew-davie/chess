     36 U009c					      ECHO	"REQUIRED SIZE =", OVERLAY_DELTA
     37 U009c					      ERR
     38 U009c					      ENDIF
     39 U009c					      LIST	ON
     40 U009c					      ECHO	OVERLAY_NAME, "-", OVERLAY_SIZE - ( * - Overlay ), "bytes available"
     41 U009c					      ENDM
