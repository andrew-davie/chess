      7 U009e							; OVERLAYS!
      8 U009e							; These variables are overlays, and should be managed with care
      9 U009e							; They co-exist (each "OVERLAY" starts at the zero-page variable "Overlay"
     10 U009e							; and thus, overlays cannot be used at the same time (that is, you cannot
     11 U009e							; use a variable in overlay #1 while at the same time using a variable in
     12 U009e							; overlay #2
     13 U009e
     14 U009e							; for clarity, prefix ALL overlay variables with double-underscore (__)
     15 U009e
     16 U009e							; TOTAL SPACE USED BY ANY OVERLAY GROUP SHOULD BE <= SIZE OF 'Overlay'
     17 U009e							; ensure this by using the VALIDATE_OVERLAY macro
     18 U009e							;---------------------------------------------------------------------------------------------------
     19 U009e
     20 U009e					      MAC	overlay
     21 U009e				   OVERLAY_NAME SET	{1}
     22 U009e					      SEG.U	OVERLAY_{1}
     23 U009e					      org	Overlay
     24 U009e					      ENDM		; {name}
     25 U009e
     26 U009e							;---------------------------------------------------------------------------------------------------
     27 U009e
     28 U009e					      MAC	validate_overlay
     29 U009e					      LIST	OFF
     30 U009e				   OVERLAY_DELTA SET	* - Overlay
     31 U009e					      IF	OVERLAY_DELTA > MAXIMUM_REQUIRED_OVERLAY_SIZE
     32 U009e				   MAXIMUM_REQUIRED_OVERLAY_SIZE SET	OVERLAY_DELTA
     33 U009e					      ENDIF
     34 U009e					      IF	OVERLAY_DELTA > OVERLAY_SIZE
     35 U009e					      ECHO	"Overlay", OVERLAY_NAME, "is too big!"
     36 U009e					      ECHO	"REQUIRED SIZE =", OVERLAY_DELTA
     37 U009e					      ERR
     38 U009e					      ENDIF
     39 U009e					      LIST	ON
     40 U009e					      ECHO	OVERLAY_NAME, "-", OVERLAY_SIZE - ( * - Overlay ), "bytes available"
     41 U009e					      ENDM
     42 U009e
     43 U009e							;---------------------------------------------------------------------------------------------------
